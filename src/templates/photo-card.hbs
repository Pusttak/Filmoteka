{{#each this}}
  <div class="photo-card">
    <a class="photo-card__link" href="{{largeImageURL}}" target="_blank" rel="noopener noreferrer">
        <img src="{{webformatURL}}" alt="{{tags}}" loading="lazy" />
        <div class="info">
          <p class="info-item">
          <b>{{likes}} Likes</b>
          </p>
          <p class="info-item">
          <b>{{views}} Views</b>
          </p>
          <p class="info-item">
          <b>{{comments}} Comments</b>
          </p>
          <p class="info-item">
          <b>{{downloads}} Downloads</b>
          </p>
        </div>
    </a>
</div>
{{/each}}