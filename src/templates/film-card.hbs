{{#each this}}
<li class="gallery__item">
           <a href="" id="{{id}}">
            <img class="gallery__img" src="https://image.tmdb.org/t/p/w500{{poster_path}}" alt="">
            <div class="gallery__item-info">
                <h3 class="gallery__item-name">
                {{title}} </h3>
                <div class="gallery__item-info-wrap">
                <p class="gallery__item-genres">
                    Drama, actions
                </p>
                <span class="gallery__item-year">|2020</span>
                <span class="gallery_item-rating">10</span>
                </div>
            </div>
           </a>
        </li>
{{/each}}